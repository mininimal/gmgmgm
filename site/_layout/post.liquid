---
title: {{ post.title }}
---
<article>
  <hgroup>
    <h1>{{ post.title }}</h1>
  </hgroup>
  <header class="projectIntro">
    <img src="{{ project.featuredMedia }}">
    <div class="introText">
      {{ project.title }}{{ project.description }}
    </div>
  </header>
  <div class="projectContent">
    {{ project.content }}
  </div>
</article>